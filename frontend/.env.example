VITE_API_URL=http://localhost:3001/api
VITE_WS_URL=ws://localhost:3001/ws

# Authentication Configuration (Optional)
# Set VITE_AUTH_ENABLED=true to require OIDC authentication
VITE_AUTH_ENABLED=false

# OIDC Configuration (Required when VITE_AUTH_ENABLED=true)
# Example values for Microsoft Entra ID:
# VITE_OIDC_AUTHORITY=https://login.microsoftonline.com/{tenant-id}/v2.0
# VITE_OIDC_CLIENT_ID={client-id}
# VITE_OIDC_REDIRECT_URI=http://localhost:5173/callback
# VITE_OIDC_SCOPE=api://{client-id}/access_as_user openid profile
VITE_OIDC_AUTHORITY=
VITE_OIDC_CLIENT_ID=
VITE_OIDC_REDIRECT_URI=
VITE_OIDC_SCOPE=api://your-client-id/access_as_user openid profile