{
  "name": "frontend",
  "private": true,
  "version": "1.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t threek8s-frontend .",
    "docker:build:multi": "docker buildx build --platform linux/amd64,linux/arm64 -t threek8s-frontend .",
    "docker:run": "docker run -p 80:80 threek8s-frontend",
    "docker:test": "docker build -t threek8s-frontend:test --target builder . && docker run --rm threek8s-frontend:test npm test"
  },
  "devDependencies": {
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "three": "^0.180.0"
  }
}
